{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prefront-deploy": "cd .. && pnpm run build-front",
    "front-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/cloudfront.ts\" deploy --all --profile produser",
    "cognito-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/cognito.ts\" deploy --all --profile produser",
    "layer-version-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/lambda-layer-version-deploy.ts\" deploy --profile produser",
    "rest-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/rest.ts\" deploy --profile produser",
    "all-deploy": "npm run layer-version-deploy && npm run rest-deploy && npm run front-deploy",
    "cdk": "cdk",
    "synth:rest": "cross-env CDK_SYNTH=true cdk -a \"npx ts-node --prefer-ts-exts bin/rest.ts\" synth rinne-circle-RineCircleRESTAPIStack",
    "prelocal-api": "npm run synth:rest",
    "local-api": "sam local start-api -t ./cdk.out/rinne-circle-RineCircleRESTAPIStack.template.json --env-vars env.json"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "20.3.1",
    "aws-cdk": "2.84.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "esbuild": "^0.18.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.84.0-alpha.0",
    "aws-cdk-lib": "2.84.0",
    "constructs": "^10.2.52",
    "fs-extra": "^11.1.1",
    "source-map-support": "^0.5.21"
  }
}