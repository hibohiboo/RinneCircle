{
  "name": "@rinne-circle/components",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ../../docs/astro/public/rinne-components",
    "clean": "rm -rf .turbo",
    "test": "jest --config ./jest.config.json",
    "mv-jest-reports": "cp ./public/mockServiceWorker.js ../../docs/astro/public/ && mv ./__reports__ ../../docs/astro/public/jest-reports"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.12.1",
    "react-syntax-highlighter": "^15.5.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rinne-circle/backend": "workspace:*",
    "@aws-sdk/s3-presigned-post": "^3.352.0",
    "@prisma/client": "^4.15.0",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-interactions": "7.0.20",
    "@storybook/addon-links": "7.0.20",
    "@storybook/addon-viewport": "^7.0.20",
    "@storybook/blocks": "7.0.20",
    "@storybook/csf": "^0.1.1",
    "@storybook/global": "^5.0.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "7.0.20",
    "@storybook/react-vite": "7.0.20",
    "@storybook/testing-library": "0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "clsx": "^1.2.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporters": "^3.1.4",
    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "prop-types": "15.8.1",
    "storybook": "7.0.20",
    "storybook-addon-react-router-v6": "^1.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "whatwg-fetch": "^3.6.2"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
