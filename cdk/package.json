{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prefront-deploy": "cd .. && pnpm run build-front",
    "front-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/cdk.ts\" deploy --all --profile produser",
    "cognito-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/cognito.ts\" deploy --all --profile produser",
    "layer-version-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/lambda-layer-version-deploy.ts\" deploy --profile produser",
    "rest-deploy": "cdk -a \"npx ts-node --prefer-ts-exts bin/rest.ts\" deploy --profile produser",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "20.2.5",
    "aws-cdk": "2.82.0",
    "dotenv": "^16.1.4",
    "esbuild": "^0.17.19",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.82.0-alpha.0",
    "aws-cdk-lib": "2.82.0",
    "constructs": "^10.2.45",
    "fs-extra": "^11.1.1",
    "source-map-support": "^0.5.21"
  }
}